{
  "lib/learnathon/web/views/*_view.ex": {
    "type": "view"
  },
  "lib/learnathon/web/templates/*.html.eex": {
    "type": "template"
  },
  "lib/learnathon/web/templates/page/*.html.eex": {
    "type": "page"
  },
  "lib/learnathon/web/templates/speaker/*.html.eex": {
    "type": "speaker"
  },
  "lib/learnathon/web/controllers/*_controller.ex": {
    "alternate": "test/web/controllers/{}_controller_test.ex",
    "type": "controller"
  },
  "test/web/controllers/*_controller_test.ex": {
    "alternate": "lib/learnathon/web/controllers/{}_controller.ex"
  },
  "lib/learnathon/submission_manager/submission_manager.ex": {
    "alternate": "test/submission_manager/submission_manager_test.exs",
    "type": "sub"
  },
  "test/submission_manager/submission_manager_test.exs": {
    "alternate": "lib/learnathon/submission_manager/submission_manager.ex"
  },
  "lib/learnathon/web/router.ex": {"type": "router"}
}

