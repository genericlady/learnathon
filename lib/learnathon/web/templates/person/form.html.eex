<%= form_for @changeset, person_path(@conn, :create), fn f -> %>
  <div class="form-group">
    <%= email_input f, :email, placeholder: "Email", class: "form-control" %>
  </div>
  <div clas="form-group">
    <%= password_input f, :password, placeholder: "Password", 
        class: "form-control" %>
  </div>
  <%= submit "Create Person", class: "btn btn-primary" %>
<% end %>
